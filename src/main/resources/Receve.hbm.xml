<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name = "POJO.Receve" table = "receve">

        <meta attribute = "class-description">
            This class contains the storage detail.
        </meta>

        <id name = "id_receve" type = "int" column = "id_receve">
            <generator class="native"/>
        </id>

        <property name = "id_reciver" column = "id_recevier" type = "int"/>
        <property name = "id_js" column = "id_js" type = "int"/>
        <property name = "ammount_rec" column = "ammount_rec" type = "int"/>
        <property name = "date_rec" column = "date_rec" type = "date"/>
        <property name = "processed" column = "processed" type = "int"/>

    </class>
</hibernate-mapping>